!###############################################################################
!
! aed_bio  :  Settings that control the coupling between TUFLOW-FV & AED
! 
!-------------------------------------------------------------------------------
&aed_bio
  !-- AED configuration flags
    solution_method     = 1                   ! Euler's method 
    do_limiter          = .true.              ! keep truckin if bad things happen
    display_minmax      = .false.             ! display to screen 
   !display_colnid      = 10                  ! ..?..
   !do_2d_atm_flux      = .true.              
   !do_particle_bgc     = .false.
   !do_zone_averaging   =
    glob_min            =  -1e38
    glob_max            =   1e38
    min_water_depth     = 0.0401
    n_equil_substep     =  12
	  longitude           = 115.50 
    latlat              = -32.25
  ! link_host_time      = .false.             ! in some cases, we cant rely on hosts time
  ! startday            = 212534151308.21100  ! from TUFLOW-FV t = 02/11/2022 00:00:00
  !-- Linkages with host model: PHYSICS
    link_bottom_drag    =  .true.
    link_surface_drag   = .false.
    link_water_density  = .false.
	  link_wave_stress    =  .true.
  !-- Linkages with host model: LIGHT
    link_ext_par        = .false.
    link_water_clarity  =  .true.
    base_par_extinction = 0.2 !Kw
    ext_tss_extinction  = .false.
    tss_par_extinction  = 0.02
  !-- Linkages with host model: RIPARIAN
    link_solar_shade    = .false.
    link_rain_loss      = .false.
  !-- Benthic variable initialisation
    !init_values_file   = '../../gis_repo/2_benthic/ecology/csiem_aed_benthic_map_A001_csiem20_MPB.csv'
    !route_table_file   = '../External/AED/Routing_Tbl.csv'
/

&aed_config
   solution_method = 1

   !# Switches for configuring model operation and active links with the host model
   link_bottom_drag = .FALSE.
   link_surface_drag = .FALSE.
   link_water_density = .FALSE.
   link_water_clarity = .FALSE.
   link_ext_par = .FALSE.
   base_par_extinction = 0.1
   ext_tss_extinction = .FALSE.
   tss_par_extinction = 0.2 
   do_particle_bgc = .FALSE.
   do_2d_atm_flux = .TRUE.
                      
   do_zone_averaging = .FALSE.
   link_solar_shade = .TRUE.
   link_rain_loss = .FALSE.

!  init_values_file = 'aed_initial'
!  output_file = 'aed_data'

   do_limiter = .FALSE.

   !# maximum single precision real is 2**128 = 3.4e38
   glob_min = -1.0e38
   glob_max =  1.0e38
   no_glob_lim = .FALSE.

   min_water_depth =  0.0401
   n_equil_substep = 1

   link_wave_stress = .FALSE.
   wave_factor =  1.0

   nir_frac =  0.52   ! 0.51
   par_frac =  0.43   ! 0.45
   uva_frac =  0.048  ! 0.035
   uvb_frac =  0.002  ! 0.005

   display_minmax = .TRUE.

!  mobility_off = .TRUE.

   depress_clutch = .FALSE.
   depress_clutch2 = .FALSE.

   debug_interval = 10
   debug_col = 111

!  ts_write_delay = 10  ! number of timesteps between writes
/

