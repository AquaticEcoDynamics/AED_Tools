/

!###############################################################################
! NCS: aed_noncohesive
!-------------------------------------------------------------------------------
&aed_noncohesive
   !-- Select SS groups and their light attenuation
    num_ss       = 1
    ss_initial   = 1,1
    Ke_ss        = 0.06, 0.063
   !-- Configure particle settling
    settling     = 1
    w_ss         = -0.08,-0.001
    d_ss         = 2e-6,1e-5
    rho_ss       = 1.5e3,1.8e3
   !-- Configure sediment resuspension
    resuspension = 1
    epsilon      = 0.003
    tau_0        = 0.03, 0.06 !0.03, 0.03
    tau_r        = 1.0
    Ktau_0       = 0.00001
    macrophyte_link_var = '' 
   !-- Configure sediment mass
    simSedimentMass = .true.
    fs              = 0.4,0.4
    sed_porosity    = 0.6
/

